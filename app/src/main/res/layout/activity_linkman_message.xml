<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
   >
    <include layout="@layout/toolbar"></include>

    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_xing_q"
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_geren_xing"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:background="@null"
            />
    </LinearLayout>

    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_ming_q"
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_geren_ming"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:background="@null"
            />
    </LinearLayout>
    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_youxiang_q"
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_geren_emale"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:background="@null"
            />
    </LinearLayout>
    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_phone_q"
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_geren_phone"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:background="@null"
            android:digits="1234567890"
            android:inputType="phone|number"
            />
    </LinearLayout>
    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_dianhua_q"
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_geren_dianhua"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:background="@null"
            />
    </LinearLayout>
    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_lianxidizhi_q"
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_lainxidizhi"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:background="@null"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        />
</LinearLayout>
