<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
<include layout="@layout/toolbar"></include>

    <LinearLayout
        android:id="@+id/ll_fahuo_province"
        style="@style/mine_tiaomu"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_title_fahuoprovince"
           style="@style/w_h_20dp"
            android:text="发货省份"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/tv_fahuo_province"
            style="@style/w_h_20dp"
            />
        <ImageView
            style="@style/right_jiantou" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_fahuo_city"
        style="@style/mine_tiaomu"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_title_fahuocity"
            style="@style/w_h_20dp"
            android:text="发货市区"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/tv_fahuo_city"
            style="@style/w_h_20dp"
            />
        <ImageView
            style="@style/right_jiantou" />
    </LinearLayout>
    <LinearLayout
    android:id="@+id/ll_shouhuo_province"
    style="@style/mine_tiaomu">
    <TextView
        android:id="@+id/tv_title_shouhuoprovince"
        style="@style/w_h_20dp"
        android:text="收货省份"
        android:layout_weight="1"
        />
    <TextView
        android:id="@+id/tv_shouhuo_province"
        style="@style/w_h_20dp"
        />
    <ImageView
        style="@style/right_jiantou" />
</LinearLayout>
    <LinearLayout
        android:id="@+id/ll_shouhuo_city"
        style="@style/mine_tiaomu">
        <TextView
            android:id="@+id/tv_title_shouhuocity"
            style="@style/w_h_20dp"
            android:text="收货市区"
            android:layout_weight="1"
            />
        <TextView
            android:id="@+id/tv_shouhuo_city"
            style="@style/w_h_20dp"
            />
        <ImageView
            style="@style/right_jiantou" />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_zhongliang"
        style="@style/mine_tiaomu">
        <TextView
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_zhongliang1"
            style="@style/w_h_20dp"
            android:layout_weight="1"
            />
        <EditText
            android:id="@+id/tv_zhongliang2"
            style="@style/w_h_20dp"
            android:layout_weight="1"
            />
        <ImageView
            style="@style/right_jiantou" />
    </LinearLayout>
    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_tianshu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:layout_weight="1"
            />
        <ImageView
            style="@style/right_jiantou" />
    </LinearLayout>
    <LinearLayout
        style="@style/mine_tiaomu">
        <TextView
            style="@style/w_h_20dp"
            />
        <EditText
            android:id="@+id/tv_feiyong"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textColor="@color/gray"
            android:gravity="right|center_vertical"
            android:layout_weight="1"
            />
        <ImageView
            style="@style/right_jiantou" />
    </LinearLayout>
    <TextView
        android:id="@+id/tv_delete_muban"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x80"
        android:layout_marginTop="15dp"
        android:background="@color/white"
        android:text="@string/delete"
        android:gravity="center"
        android:textColor="@color/hand"
        android:textSize="@dimen/font_18sp"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="6"></View>
</LinearLayout>
