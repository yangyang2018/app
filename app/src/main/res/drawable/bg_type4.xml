<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:color="@color/hand" android:state_checkable="true">
        <shape>
            <stroke android:width="1dp" android:color="@color/hand" />
            <corners android:radius="3dp" />
        </shape>
    </item>
    <!--<item android:color="@color/hand" android:state_focused="true" />-->
    <!--<item android:color="@color/hand" android:state_pressed="true" />-->
    <item android:color="@color/text_dark"  android:state_checked="false">
        <shape>
            <stroke android:width="1dp" android:color="@color/hand" />
            <corners android:radius="3dp" />
        </shape>
    </item>
</selector>